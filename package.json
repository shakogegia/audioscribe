{
  "name": "audioscribe",
  "version": "0.0.12",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run next:dev\" \"npm run chroma\" \"npm run jobs\"",
    "build": "npm run next:build",
    "start": "concurrently \"npm run next:start\" \"npm run chroma\" \"npm run jobs\"",
    "jobs": "ts-node --project workers/tsconfig.json ./scripts/start-jobs.ts",
    "next:dev": "next dev --turbopack",
    "next:build": "next build --turbopack",
    "next:start": "next start",
    "lint": "eslint",
    "chroma": "chroma run --path ./data/chromadb",
    "script:vectorize": "dotenv ts-node --project workers/tsconfig.json workers/vectorize.ts",
    "script:transcribe": "dotenv ts-node --project workers/tsconfig.json workers/transcribe.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "release": "dotenv release-it"
  },
  "dependencies": {
