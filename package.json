{
  "name": "audioscribe",
  "version": "0.5.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run web\" \"npm run chroma\" \"npm run workers\"",
    "start": "NODE_ENV=production node --import=tsx server.ts",
    "build": "next build",
    "lint": "eslint",
    "web": "nodemon --config nodemon.web.json",
    "workers": "nodemon --config nodemon.workers.json",
    "chroma": "chroma run --path ./data/chromadb",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "release": "dotenv release-it",
    "publish": "docker buildx build --platform linux/amd64,linux/arm64 --push .  -t shakogegia/audioscribe"
  },
