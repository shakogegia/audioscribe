{
  "name": "audioscribe",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"nodemon\" \"npm run chroma\"",
    "start": "NODE_ENV=production node --import=tsx server.ts",
    "build": "next build",
    "lint": "eslint",
    "chroma": "chroma run --path ./data/chromadb",
    "script:vectorize": "node --import=tsx src/server/workers/vectorize.ts",
    "script:transcribe": "node --import=tsx src/server/workers/transcribe.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "release": "dotenv release-it",
    "publish": "docker buildx build --platform linux/amd64,linux/arm64 --push .  -t shakogegia/audioscribe"
  },
